# Ignore Python virtual environments
.venv/
venv/
ENV/
env/

# Ignore Python compiled files
__pycache__/
*.py[cod]
*$py.class
*.so
.Python

# Ignore Jupyter Notebook checkpoints
.ipynb_checkpoints/

# Ignore IDE/Editor files
.vscode/
.idea/
*.swp
*.swo
*~
.DS_Store

# Ignore model files (large binary files)
models/*.pkl
models/*.joblib
models/*.h5
models/*.pt
